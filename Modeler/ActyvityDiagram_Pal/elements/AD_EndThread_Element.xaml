<UserControl x:Class="ActyvityDiagram_Pal.AD_EndThread_Element"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:Controls="clr-namespace:DiagramDesigner.Controls;assembly=DiagramDesigner" mc:Ignorable="d" Height="40" Width="40">
    <Grid Margin="2">
        <Rectangle Fill="Transparent" ></Rectangle>
        <Ellipse Height="32" Width="32" Fill="Black"></Ellipse>
        <Ellipse Height="30" Width="30" Fill="White"></Ellipse>
        <Path  Name="MainBorder" Stroke="Black">
            <Path.Data>M 10,10 L 25 25 Z</Path.Data>
        </Path>
        <Path Stroke="Black">
            <Path.Data>M 25,10 L 10 25 Z</Path.Data>
        </Path>
        
    </Grid>
</UserControl>
